	<analysis id="phylo-major" type="paup">
		<identify>
			1,2,3,4,5,6,7
		</identify>
		<cutoff>
			70
		</cutoff>
		<block>
			begin paup;
			log file=paup.log replace=yes;
			exclude gapped;
			export format=nexus file=paup.nex replace=yes;
			outgroup 7a_EF108306;
			set criterion=distance outroot=monophyl;
			dset distance=HKY NegBrLen=SetAbsVal;
			NJ;
			savetree format=nexus brlens=yes file=paup.tre replace=yes;
			bootstrap nreps=100 search=nj;
			end;
			quit;
		</block>
		<options>
			log,alignment,tree
		</options>
	</analysis>
	
	<analysis id="phylo-major-scan" type="scan">
		<minimum-length>
			500
		</minimum-length>
		<window>
			400
		</window>
		<step>
			100
		</step>
		<cutoff>
			0.9
		</cutoff>
		<analysis type="paup">
			<identify>
				1,2,3,4,5,6,7
			</identify>
			<cutoff>
				70
			</cutoff>
			<block>
				begin paup;
				log file=paup.log replace=yes;
				exclude gapped;
				export format=nexus file=paup.nex replace=yes;
				outgroup 7a_EF108306;
				set criterion=distance outroot=monophyl;
				dset distance=HKY NegBrLen=SetAbsVal;
				constraints CONSTRE=${BACKBONE-CLUSTERS};
				NJ constraints=CONSTRE enforce=yes;
				savetree format=nexus brlens=yes file=paup.tre replace=yes;
				bootstrap nreps=10 search=nj;
				end;
				quit;
			</block>
		</analysis>
		<options>data-all</options>
	</analysis>
	
