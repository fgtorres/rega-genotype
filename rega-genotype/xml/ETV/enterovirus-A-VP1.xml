	<analysis id="phylo-serotype" type="paup">
		<identify>
		  CV-A2, CV-A4, CV-A5, CV-A6, CV-A7, CV-A8, CV-A10, CV-A12,
		  CV-A14, CV-A16, EV-71[0;9], EV-76, EV-89, EV-90, EV-91, SV19, SV46,
		  CV-A3, EV-92, SV43, outgroup
		</identify>
		<cutoff>
			70
		</cutoff>
		<block>
			begin paup;
			log file=paup.log replace=yes;
			export format=phylip file=paup.fasta replace=yes;
			export format=nexus file=paup.nex replace=yes;
			outgroup Outgroup|NC_001430;
			set criterion=distance outroot=monophyl;
			dset distance=TamNei rates=gamma shape=0.393955 pinvar=0 NegBrLen=SetAbsVal;
			constraints
			CONSTRE(BACKBONE)=${BACKBONE-CLUSTERS};
			NJ constraints=CONSTRE enforce=yes;
			savetree format=nexus brlens=yes file=paup.tre replace=yes;
			bootstrap nreps=100 search=nj;
			end;
			quit;
		</block>
		<options>
			log,alignment,tree,inner,outer
		</options>
	</analysis>

	<analysis id="phylo-VP1-EV-71" type="paup">
		<identify>
			EV-71_Br, 
			EV-71_B0, EV-71_B1, EV-71_B2, EV-71_B3, EV-71_B4, EV-71_B5,
			EV-71_C1, EV-71_C2, EV-71_C3, EV-71_C4, EV-71_C5,
			CV-A16[1]
		</identify>
		<cutoff>
			70
		</cutoff>
		<block>
			begin paup;
			log file=paup.log replace=yes;
			export format=phylip file=paup.fasta replace=yes;
			exclude gapped;
			export format=nexus file=paup.nex replace=yes;
			outgroup CV_A16|U05876;
			set criterion=distance;
			dset distance=HKY85 pinvar=0.44 NegBrLen=SetAbsVal;
			constraints CONSTRE(BACKBONE)=${BACKBONE-CLUSTERS};
			NJ constraints=CONSTRE enforce=yes;
			savetree format=nexus brlens=yes file=paup.tre replace=yes;
			bootstrap nreps=100 search=nj;
			end;
			quit;
		</block>
		<options>
			log,alignment,tree,inner,outer
		</options>
	</analysis>

	<analysis id="scan-self" type="scan">
		<window>
			300
		</window>
		<step>
			50
		</step>
		<analysis type="paup">
			<identify>
				CV-A2
			</identify>
			<block>
				begin paup;
				log file=paup.log replace=yes;
				exclude gapped;
				export format=nexus file=paup.nex replace=yes;
				outgroup EU360814|III.1;
				set criterion=distance outroot=monophyl;
				dset distance=TamNei rates=gamma shape=0.413123 NegBrLen=SetAbsVal;
				NJ;
				savetree format=phylip brlens=yes file=paup.tre replace=yes;
				bootstrap nreps=100 search=nj;
				end;
				quit;
			</block>
		</analysis>
	</analysis>

