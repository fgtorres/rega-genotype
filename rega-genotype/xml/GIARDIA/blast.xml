<genotype-analyses>
  <alignment file="blast.fasta" data="dna"/>

  <clusters>
    <cluster id="16S" name="16S">
      <taxus name="16S" />
     	<!-- length: 1454
           	 img: 0 - 1454
         -->
    </cluster>

    <cluster id="B-giardin" name="B-giardin">
      <taxus name="B-giardin" />
     	<!-- length: 819
           	 img: 1602 - 2421
         -->
    </cluster>

    <cluster id="GDH" name="GDH">
      <taxus name="GDH" />
         <!-- length: 1350
           	  img: 2550 - 3900
          -->
    </cluster>

    <cluster id="TPI" name="TPI">
      <taxus name="TPI" />
      	 <!-- length: 774
           	  img: 4049 - 4823    	 
      	  -->
    </cluster>
  </clusters>

  <analysis id="blast" type="blast">
    <identify>
      16S, B-giardin, GDH, TPI
    </identify>

    <cutoff>
      100
    </cutoff>

    <options>
      -q -1 -r 1
    </options>
  </analysis>
</genotype-analyses>

