<genotype-analyses>
	
	<alignment file="chikv.fasta" trim="true" />

	<clusters>
		
		<cluster id="1" name="Asian and Caribbean">
			<taxus name="Asian_HM045813_India_1963" />
			<taxus name="Asian_EF027141_India_1973" />
			<taxus name="Asian_EF027140_India_1963" />
			<taxus name="Asian_FN295483_Malaysia_2006" />
			<taxus name="Asian_FJ807897_Taiwan_2007" />
			<taxus name="Asian_HM045790_Phil_1985" />
		</cluster>

		<cluster id="2" name="East-Central-South-African">
			<taxus name="ECSA_IN_AB455494_Japan_2006" />
			<taxus name="ECSA_IN_EU244823_Italy_2007" />
			<taxus name="ECSA_IN_FJ445426_SriLanka_2008" />
			<taxus name="ECSA_IN_FN295485_Malaysia_2008" />
			<taxus name="ECSA_IN_GU199352_China_2008" />
			<taxus name="ECSA_IN_GU301781_Thailand_2009" />
			<taxus name="ESCA_IOC_AM258993_Reunion_2005" />
			<taxus name="ESCA_IOC_EF012359_Mauri_2006" />
			<taxus name="ECSA_IOC_AM258991_Seyche_2005" />
			<taxus name="ECSA_HM045784_Central" />
			<taxus name="ECSA_HM045822_Central" />
		</cluster>
		
		<cluster id="3" name="East-Central-South-African">
			<taxus name="ECSA_HM045792_South" />
			<taxus name="ECSA_HM045811_Tanzania_1953" />
			<taxus name="ECSA_HM045821_Senegal_1963" />
		</cluster>

		<cluster id="4" name="West African">
			<taxus name="WestAfr_HM045785_Senegal_1966" />
			<taxus name="WestAfr_HM045815_Senegal_1979" />
			<taxus name="WestAfr_HM045818_Ivory" />
			<taxus name="WestAfr_HM045820_Ivory" />
			<taxus name="WestAfr_HM045817_Senegal_2005" />
			<taxus name="WestAfr_HM045786_Nigeria_1964" />
		</cluster>

	</clusters>
	
	<analysis id="phylo-major" type="paup">
		<identify>
			1,2,3,4
		</identify>
		<cutoff>
			700
		</cutoff>
		<block>
			begin paup;
			log file=paup.log replace=yes;
			exclude gapped;
			export format=nexus file=paup.nex replace=yes;
			set criterion=distance rootmethod=midpoint;
			set rootmethod=midpoint;
			dset distance=HKY NegBrLen=SetAbsVal;
			constraints CONSTRE=${BACKBONE-CLUSTERS};
			NJ constraints=CONSTRE;
			savetree format=nexus brlens=yes file=paup.tre replace=yes;
			bootstrap nreps=1000 search=nj;
			end;
			quit;
		</block>
		<options>
			log,alignment,tree,inner,outer
		</options>
	</analysis>

	<analysis id="phylo-major-1-self-scan" type="scan">
		<window>
			1000
		</window>
		<step>
			500
		</step>
		<analysis type="paup">
			<identify>
				1,2,3,4
			</identify>
			<cutoff>
				70
			</cutoff>
			<block>
				begin paup;
				log file=paup.log replace=yes;
				exclude gapped;
				export format=nexus file=paup.nex replace=yes;
				set rootmethod=midpoint;
				set criterion=distance rootmethod=midpoint;
				dset distance=HKY NegBrLen=SetAbsVal;
				NJ; 
				savetree format=phylip brlens=yes file=paup.tre replace=yes;
				bootstrap nreps=100 search=nj;
				end;
				quit;
			</block>
		</analysis>
	</analysis>

</genotype-analyses>
	